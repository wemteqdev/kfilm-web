{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-cors": "^0.11.0",
        "bensampo/laravel-enum": "^1.10",
        "cviebrock/eloquent-sluggable": "^4.5",
        "cybercog/laravel-love": "^5.1",
        "cyrildewit/eloquent-viewable": "^2.4.0",
        "doctrine/dbal": "~2.3",
        "fideloper/proxy": "^4.0",
        "laracasts/flash": "^3.0",
        "laravel/cashier": "~7.0",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6.0",
        "predis/predis": "^1.1",
        "prettus/l5-repository": "^2.6",
        "rtconner/laravel-tagging": "~2.2",
        "spatie/laravel-model-status": "^1.6",
        "spatie/laravel-permission": "^2.17",
        "spatie/laravel-responsecache": "^4.4",
        "spatie/laravel-sitemap": "^5.2",
        "spatie/laravel-stripe-webhooks": "^1.1",
        "vimeo/laravel": "^5.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
